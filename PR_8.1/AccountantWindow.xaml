<Window x:Class="PR_8._1.AccountantWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR_8._1"
        mc:Ignorable="d"
        Title="AccountantWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="197*"/>
        </Grid.RowDefinitions>
        <Menu Background="White" HorizontalAlignment="Center" Width="800" Height="30" VerticalAlignment="Center">
            <MenuItem Header="Отчет" >
                <MenuItem.Icon>
                    <Image Source="/Images/Отчет.png" />
                </MenuItem.Icon>
                <MenuItem Header="Создать отчет" />
                <MenuItem Header="Поиск отчета" />
            </MenuItem>
            <MenuItem Header="Экспорт отчета">
                <MenuItem.Icon>
                    <Image Source="/Images/Экспорт.png" />
                </MenuItem.Icon>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
